
public class JavaPractice1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Find missing number from an array of 10 integers
		int sum,sum1=0;
		int[] a = {4,6,5,8,7,9,1,2,3};
		int i = a.length;
		sum = ((i+1)(i+2))/2;
		for(int j=0; j<i;j++)
		{
			sum- =i[j];
		}
		System.out.println(sum);
	}

}
